<h1>新規登録</h1>
<%= form_with model: @post, local: true do |form| %>
  <div>
    <%= form.label :place %>
    <%= form.text_field :place %>
  </div>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <div>
    <%= form.label :fix_detail %>
    <%= form.text_field :fix_detail %>
  </div>
  <div>
    <%= form.submit "登録" %>
  </div>
<% end %>
